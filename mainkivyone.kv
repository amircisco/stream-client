# -*- coding: utf-8 -*-
<WindowManager>
    Tizer
        name: "Tizer"
        BoxLayout:
            orientation:"vertical"
            size:self.parent.width,self.parent.height
            VideoPlayerNoCrl
                source:"none.jpg"
                width:self.parent.width
                height:self.parent.height

    KivyShowVideoFrames
        name: "KivyShowVideoFrames"
        BoxLayout:
            orientation : "horizontal"
            size:root.width,root.height

            BoxLayout:
                orientation: "vertical"
                size_hint:.250,1

                Label:
                    size_hint:1,.01
                BoxLayout:
                    orientation: "horizontal"
                    size_hint:1,.04

                    Label:
                        size_hint_x: .01
                    Button:
                        size_hint_x: .24
                        id:btn_setting
                        font_name:"Assets/Fonts/BKoodakBold.ttf"
                        font_size:22
                        on_press:self.parent.parent.parent.parent.clicked_setting()

                    Button:
                        size_hint_x: .24
                        id:btn_history
                        font_name:"Assets/Fonts/BKoodakBold.ttf"
                        font_size:22
                        on_press:self.parent.parent.parent.parent.clicked_history()

                    Button:
                        size_hint_x: .24
                        id:btn_dissconnect
                        font_name:"Assets/Fonts/BKoodakBold.ttf"
                        font_size:22
                        on_press:self.parent.parent.parent.parent.clicked_dissconnect()

                    Button:
                        size_hint_x: .24
                        id:btn_connect
                        font_name:"Assets/Fonts/BKoodakBold.ttf"
                        font_size:22
                        on_press:self.parent.parent.parent.parent.clicked_connect()

                    Label:
                        size_hint_x: .01

                Label:
                    size_hint:1,.01
                BoxLayout:
                    orientation:"vertical"
                    size_hint:1,.94
                    GridLayout:
                        cols:3
                        rows:1
                        size_hint:1,1
                        id:parent_grid_tbls
                        opacity:0

                        BoxLayout:
                            orientation:"vertical"
                            Label:
                                size_hint:1,.03
                                font_size:22
                                font_name:"Assets/Fonts/BKoodakBold.ttf"
                                id:label_pelak


                            ScrollView:
                                GridLayout:
                                    id:data_grid_pelak
                                    orientation: "vertical"
                                    size_hint_y: None
                                    height: self.minimum_height
                                    row_default_height: self.parent.parent.width
                                    cols:1


                        BoxLayout:
                            orientation:"vertical"
                            Label:
                                size_hint:1,.03
                                id:label_face
                                font_size:22
                                font_name:"Assets/Fonts/BKoodakBold.ttf"

                            ScrollView:
                                GridLayout:
                                    id:data_grid_face
                                    orientation: "vertical"
                                    size_hint_y: None
                                    height: self.minimum_height
                                    row_default_height: self.parent.parent.width
                                    cols:1


